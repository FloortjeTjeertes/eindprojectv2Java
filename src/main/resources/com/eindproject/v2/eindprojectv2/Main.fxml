<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane  maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
         prefHeight="400.0" prefWidth="600.0" stylesheets="@Main.css" tabClosingPolicy="UNAVAILABLE"
         xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
         fx:controller="com.eindproject.v2.eindprojectv2.MainController">
    <Tab text="Lending/recieving">
        <fx:include  source="LendReceive.fxml"/>
    </Tab>
    <Tab onSelectionChanged="#update" text="collection">
        <fx:include fx:id="CollectionTab" source="Collection.fxml"/>

    </Tab>
    <Tab text="Members">
        <fx:include fx:id="MembersTab" source="Member.fxml"/>

    </Tab>

</TabPane>
